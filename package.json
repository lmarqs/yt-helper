{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@metamodules/postgres": "^1.2.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "deploy-node-app": "^1.4.3",
    "dotenv": "^8.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-spawn": "^6.0.5",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "react-app-rewire-workbox": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.4.1",
    "typescript": "^3.4.5",
    "workbox-webpack-plugin": "^3.0.0"
  },
  "scripts": {
    "start": "bash bin/dev.sh",
    "build": "react-app-rewired build",
    "deploy": "deploy-node-app",
    "test": "react-app-rewired test",
    "eject-www": "react-app-rewired eject",
    "psql": "export $(cat .env | xargs) && PGPASSWORD=${POSTGRES_PASSWORD} psql -h ${POSTGRES_SERVICE_HOST} -p ${POSTGRES_SERVICE_PORT} -U ${POSTGRES_USER} ${POSTGRES_DB}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "deploy-node-app": {
    "prod": {
      "port": 4000,
      "protocol": "http",
      "entrypoint": "src/api/index.js",
      "type": "spa"
    },
    "dev": {
      "port": 4000,
      "protocol": "http",
      "entrypoint": "src/api/index.js",
      "registry": "",
      "type": "spa",
      "isPublic": false
    }
  },
  "proxy": "http://localhost:4000"
}
